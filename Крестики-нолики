                    print(pole[j][i], end=".")
                print()

        def Printer_NPC_move(step, move):
            pole[row_number.index(move[1])][column_name.index(move[0])] = ind_npc
            print(f"\n–•–æ–¥ {step} (NPC): {move}")
            print(" ", end=".")
            for i in range(size):
                print(column_name[i], sep=".", end=".")
            print()
